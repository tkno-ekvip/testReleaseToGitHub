{attribute 'reflection'}
FUNCTION_BLOCK INTERNAL LWordAssertions EXTENDS AbstactAssertion IMPLEMENTS ILWordAssertions
VAR
END_VAR
VAR CONSTANT
	(* least significant bit *)
	LSB	:LWORD := 16#0000_0000_0000_0001;
	(* most significant bit *)
	MSB :LWORD := 16#8000_0000_0000_0000;
END_VAR]]></Declaration>
