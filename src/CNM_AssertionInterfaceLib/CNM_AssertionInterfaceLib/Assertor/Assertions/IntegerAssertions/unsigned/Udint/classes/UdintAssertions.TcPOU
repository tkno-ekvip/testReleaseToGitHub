METHOD valueIsGreaterThan
VAR_INPUT
	(* current value to check *)
	currentValue	:UDINT;
	(* less than lower limit *)
	lessValue		:UDINT;
	(* message if the assertion is false *)
	message			:AssertMessage;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (currentValue <= lessValue) THEN
