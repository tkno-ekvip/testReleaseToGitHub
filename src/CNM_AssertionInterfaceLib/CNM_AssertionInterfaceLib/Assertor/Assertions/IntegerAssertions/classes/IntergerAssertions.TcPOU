methods aand properties
========================

*)

{attribute 'reflection'}
FUNCTION_BLOCK INTERNAL IntergerAssertions EXTENDS CNM_AbstractObject.Object IMPLEMENTS IIntergerAssertions, IAssertCallBacksetter
VAR
	(* class to handle assert statements for signed integers *)
	signedIntegerAssertions		:SignedIntegerAssertions();
	(* class to handle assert statements for unsigned integers *)
	unsignedIntegerAssertions	:UnsignedIntegerAssertions();
END_VAR
]]></Declaration>
