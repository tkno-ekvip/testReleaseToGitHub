FUNCTION_BLOCK INTERNAL FloatAssertions EXTENDS CNM_AbstractObject.Object IMPLEMENTS IFloatAssertions, IAssertCallBacksetter
VAR
	(* class to handle assert statements for 32 bit floats *)
	realAssertions	:RealAssertions();
	(* class to handle assert statements for 64 bit floats *)
	lRealAssertions	:LRealAssertions();
END_VAR
]]></Declaration>
