| SPDX-License-Identifier: Apache-2.0
| For details check: Apache-2.0_

.. _Apache-2.0: https://www.apache.org/licenses/LICENSE-2.0

.. </legal notes>

methods and properties
=======================

*)

{attribute 'reflection'}
FUNCTION_BLOCK INTERNAL Assertions EXTENDS CNM_AbstractObject.Object IMPLEMENTS IAssertions, IAssertCallBacksetter
VAR
	(* class to handle binary assertions *)
	binaryAssertions	:BinaryAssertions();
	(* class to handle bit number assertions *)
	bitNumberAssertions	:BitNumberAssertions();
	(* class to handle boolean assertions *)
	booleanAssertions	:BooleanAssertion();
	(* class to handle date and time assertions *)
	dateTimeAssertions	:DateAndTimeAssertions();
	(* class to handle float assertions *)
	floatAssertions		:FloatAssertions();
	(* class to handle integer assertions *)
	intgerAssertions	:IntergerAssertions();
	(* class to handle object assertions *)
	objectAssertions	:ObjectAssertions();
	(* class to handle pointer assertions *)
	pointerAssertions	:PointerAssertions();
	(* class to handle string assertions *)
	stringAssertions	:StringAssertions();
	(* class to handle time assertions *)
	timeAssertions		:TimeAssertions();
END_VAR
]]></Declaration>
